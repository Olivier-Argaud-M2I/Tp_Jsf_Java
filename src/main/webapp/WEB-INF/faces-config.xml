<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">

    <navigation-rule>

        <from-view-id>/*</from-view-id>

        <navigation-case>
            <from-outcome>premierePage</from-outcome>
            <to-view-id>/page1.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>deuxiemePage</from-outcome>
            <to-view-id>/page2.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>troisiemePage</from-outcome>
            <to-view-id>/page3.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>quatriemePage</from-outcome>
            <to-view-id>/pages/page4.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>cinquiemePage</from-outcome>
            <to-view-id>/pages/page5.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>accueil</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect />
        </navigation-case>

        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/admin/pageAdmin.xhtml</to-view-id>
            <redirect />
        </navigation-case>

    </navigation-rule>


